{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters"
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "firstName": {
            "type": "varchar",
            "required": true,
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "lastName": {
            "type": "varchar",
            "required": true,
            "maxLength": 50,
            "options": [],
            "isCustom": true
        },
        "phone": {
            "type": "varchar",
            "required": true,
            "maxLength": 50,
            "options": [],
            "isCustom": true
        },
        "email": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "indexes": {
            "type": "array",
            "storeArrayValues": true,
            "noEmptyString": true,
            "default": [],
            "isCustom": true,
            "displayAsList": true
        },
        "event": {
            "type": "link"
        },
        "tickets": {
            "type": "linkMultiple",
            "layoutDetailDisabled": false,
            "layoutMassUpdateDisabled": false,
            "layoutListDisabled": false,
            "noLoad": false,
            "importDisabled": false,
            "exportDisabled": false,
            "customizationDisabled": false,
            "isCustom": true
        },
        "bunchId": {
            "type": "varchar",
            "maxLength": 150,
            "options": [],
            "tooltip": true,
            "isCustom": true
        },
        "screenshot": {
            "type": "file",
            "maxFileSize": 2,
            "accept": [
                "image/*"
            ],
            "sourceList": [],
            "isCustom": true,
            "audited": true
        },
        "bookedStart": {
            "notNull": false,
            "type": "datetime",
            "default": "javascript: return this.dateTime.getNow(1);",
            "minuteStep": 30,
            "tooltip": true,
            "isCustom": true
        },
        "bookedEnd": {
            "notNull": false,
            "type": "datetime",
            "default": "javascript: return this.dateTime.getDateTimeShiftedFromNow(1, 'days', 15);",
            "minuteStep": 30,
            "tooltip": true,
            "isCustom": true
        },
        "contact": {
            "type": "link"
        },
        "price": {
            "type": "currency",
            "tooltip": true,
            "isCustom": true
        },
        "salesDate": {
            "notNull": false,
            "type": "date",
            "isCustom": true,
            "useNumericFormat": true
        },
        "paymentMethod": {
            "type": "enum",
            "options": [
                "cash",
                "iban"
            ],
            "style": {
                "cash": null,
                "iban": null
            },
            "default": "cash",
            "isCustom": true
        },
        "status": {
            "type": "enum",
            "options": [
                "booked",
                "paid",
                "notPaid"
            ],
            "default": "booked",
            "style": {
                "booked": "warning",
                "paid": "success",
                "notPaid": null
            },
            "isCustom": true
        },
        "isTicketsSend": {
            "notNull": true,
            "type": "bool",
            "tooltip": true,
            "isCustom": true
        },
        "sendEmailTrigger": {
            "notNull": true,
            "type": "bool",
            "tooltip": true,
            "isCustom": true
        },
        "formatedPlaces": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "event": {
            "type": "belongsTo",
            "foreign": "ticketsBunchs",
            "entity": "Event",
            "audited": false,
            "isCustom": true
        },
        "tickets": {
            "type": "hasMany",
            "foreign": "ticketsBunch",
            "entity": "Ticket",
            "audited": false,
            "isCustom": true
        },
        "screenshot": {
            "type": "belongsTo",
            "entity": "Attachment",
            "skipOrmDefs": true,
            "utility": true
        },
        "contact": {
            "type": "belongsTo",
            "foreign": "ticketsBunchs",
            "entity": "Contact",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc",
        "textFilterFields": [
            "name",
            "firstName",
            "lastName",
            "phone",
            "email"
        ],
        "fullTextSearch": false,
        "countDisabled": false
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    },
    "optimisticConcurrencyControl": false
}